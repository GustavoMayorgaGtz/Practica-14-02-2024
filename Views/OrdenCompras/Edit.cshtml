@model Practica_14_02_2024.Models.OrdenCompra

@{
    ViewData["Title"] = "Edit";
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8"/>
    <title>@ViewData["Title"]</title>
    <!-- Vincula tu hoja de estilos -->
    <link href="~/css/edit.css" rel="stylesheet" type="text/css"/>
</head>
<body>
<h1>Editando orden de compra</h1>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <div class="group">
                <label asp-for="Proyecto" class="control-label"></label>
                <input asp-for="Proyecto" class="form-control" />
                <hr />
            </div>
            <div class="group">
                <label asp-for="Status" class="control-label"></label>
                <input asp-for="Status" class="form-control" />
                <hr />
            </div>
            <div class="group">
                <label asp-for="NoConsecutivo" class="control-label"></label>
                <input asp-for="NoConsecutivo" class="form-control" />
                <hr />
            </div>
            <div class="group">
                <label asp-for="Referencia" class="control-label"></label>
                <input asp-for="Referencia" class="form-control" />
                <hr />
            </div>
            <div class="group">
                <label asp-for="FechaRegistro" class="control-label"></label>
                <input asp-for="FechaRegistro" class="form-control" />
                <hr />
            </div>
            <div class="group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a class="btnBack" asp-action="Index">Back to List</a>
</div>
</body>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
